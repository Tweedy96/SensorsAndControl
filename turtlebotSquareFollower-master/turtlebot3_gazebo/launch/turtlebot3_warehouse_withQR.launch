<launch>
  <arg name="model" default="$(env TURTLEBOT3_MODEL)" doc="model type [burger, waffle, waffle_pi]"/>
  <arg name="x_pos" default="0.0"/>
  <arg name="y_pos" default="0.0"/>
  <arg name="z_pos" default="0.1"/>

  <include file="$(find gazebo_ros)/launch/empty_world.launch">
    <arg name="world_name" value="$(find turtlebot3_gazebo)/worlds/empty.world"/>
    <arg name="paused" value="false"/>
    <arg name="use_sim_time" value="true"/>
    <arg name="gui" value="true"/>
    <arg name="headless" value="false"/>
    <arg name="debug" value="false"/>
  </include>

  <param name="robot_description" command="$(find xacro)/xacro --inorder $(find turtlebot3_description)/urdf/turtlebot3_$(arg model).urdf.xacro" />

  <node pkg="gazebo_ros" type="spawn_model" name="spawn_urdf" args="-urdf -model turtlebot3_$(arg model) -x $(arg x_pos) -y $(arg y_pos) -z $(arg z_pos) -param robot_description" />

  <include file="$(find turtlebot3_gazebo)/launch/spawn_sdf.launch">
     <arg name="robot_name" value="target_box" />
     <arg name="x" value="4.0" />
     <arg name="y" value="2.0" />
     <arg name="z" value="1.0" />
     <arg name="roll" value="0"/>
     <arg name="pitch" value="0"/>
     <arg name="yaw" value="0.0" />
     <arg name="sdf_robot_file" value="$(find turtlebot3_gazebo)/models/target_box/model.sdf" />
</include>

<!-- From here -->
  <include file="$(find turtlebot3_gazebo)/launch/spawn_clutterA.launch">
     <arg name="robot_name" value="ClutteringA" />
     <arg name="x" value="5.0" />
     <arg name="y" value="0.0" />
     <arg name="z" value="-0.01" />
     <arg name="roll" value="0"/>
     <arg name="pitch" value="0"/>
     <arg name="yaw" value="0.0" />
     <arg name="sdf_robot_file" value="$(find turtlebot3_gazebo)/models/aws_robomaker_warehouse_ClutteringA_01/model.sdf" />
</include>

<include file="$(find turtlebot3_gazebo)/launch/spawn_shelfD.launch">
     <arg name="robot_name" value="ShelfD" />
     <arg name="x" value="4.35" />
     <arg name="y" value="3.2" />
     <arg name="z" value="0" />
     <arg name="roll" value="0"/>
     <arg name="pitch" value="0"/>
     <arg name="yaw" value="0.0" />
     <arg name="sdf_robot_file" value="$(find turtlebot3_gazebo)/models/aws_robomaker_warehouse_ShelfD_01/model.sdf" />
</include>


<include file="$(find turtlebot3_gazebo)/launch/spawn_ground.launch">
     <arg name="robot_name" value="Ground" />
     <arg name="x" value="0.0" />
     <arg name="y" value="0.0" />
     <arg name="z" value="-0.02" />
     <arg name="roll" value="0"/>
     <arg name="pitch" value="0"/>
     <arg name="yaw" value="0.0" />
     <arg name="sdf_robot_file" value="$(find turtlebot3_gazebo)/models/aws_robomaker_warehouse_GroundB_01/model.sdf" />
</include>




<include file="$(find turtlebot3_gazebo)/launch/spawn_Trashcan.launch">
     <arg name="robot_name" value="Trashcan" />
     <arg name="x" value="3.0" />
     <arg name="y" value="-3.0" />
     <arg name="z" value="-0.103309" />
     <arg name="roll" value="0"/>
     <arg name="pitch" value="0"/>
     <arg name="yaw" value="0.0" />
     <arg name="sdf_robot_file" value="$(find turtlebot3_gazebo)/models/aws_robomaker_warehouse_TrashCanC_01/model.sdf" />
</include>
</launch>
